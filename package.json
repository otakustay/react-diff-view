{
  "name": "react-diff-view",
  "version": "1.6.2",
  "description": "A git diff component to consume the git unified diff output.",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config=demo/webpack.config.js",
    "clean": "rm -r cjs es style",
    "build": "npm run clean && NODE_ENV=production node rollup.js",
    "build-pages": "rm -r dist && npm run build && webpack --config=demo/webpack.config.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint src demo"
  },
  "author": "otakustay",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "diff-match-patch": "^1.0.1",
    "gitdiff-parser": "0.0.8",
    "warning": "^4.0.1"
  },
  "devDependencies": {
    "@ecomfe/eslint-config": "^1.1.3",
    "antd": "^3.8.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^23.4.2",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-infinite-scroller": "^1.2.0",
    "react-markdown": "^3.4.1",
    "react-test-renderer": "^16.4.2",
    "react-timeago": "^4.1.9",
    "react-whether": "^1.0.1",
    "refractor": "^2.6.0",
    "reselect": "^3.0.1",
    "rollup": "^0.64.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1",
    "sha1": "^1.1.1",
    "shallow-changes": "^1.0.2",
    "short-hash": "^1.0.0",
    "style-loader": "^0.22.1",
    "svg-react-loader": "^0.4.5",
    "unidiff": "0.0.4",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "worker-loader": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "prop-types": ">=15.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otakustay/react-diff-view.git"
  },
  "keywords": [
    "git",
    "github",
    "diff",
    "git-diff",
    "react",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/otakustay/react-diff-view/issues"
  },
  "homepage": "https://github.com/otakustay/react-diff-view#readme",
  "files": [
    "es",
    "cjs",
    "style"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "verbose": true,
    "roots": [
      "./test"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 78,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFiles": [
      "./test/setupTest"
    ]
  }
}
